  BEGIN
    BLOCK "040904b0"
    BEGIN
      VALUE "CompanyName",      "The fetch library, https://curl.se/\0"
      VALUE "FileDescription",  "libfetch Shared Library\0"
      VALUE "FileVersion",      LIBFETCH_VERSION "\0"
      VALUE "InternalName",     "libfetch\0"
      VALUE "OriginalFilename", "libfetch.dll\0"
      VALUE "ProductName",      "The fetch library\0"
      VALUE "ProductVersion",   LIBFETCH_VERSION "\0"
      VALUE "LegalCopyright",   "Copyright (C) " LIBFETCH_COPYRIGHT "\0"
      VALUE "License",          "https://curl.se/docs/copyright.html\0"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 1200
  END
END
